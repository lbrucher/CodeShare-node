!!!
html
	head
		title CodeShare
		link(rel='stylesheet', href='/stylesheets/style2.css')
	body
		#container
			#header
				ul
					li.left
						h1 CodeShare
					- if (typeof currentUser !== 'undefined')
						li.right.signout
							.divider |
							a#logout(href='/interviewer/logout') Sign Out
					- if (typeof session != 'undefined')
						li.right Session #{session.id}
			#content
				!= body

